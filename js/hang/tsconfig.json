{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"outDir": "dist",
		"rootDir": "./src",
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		"src/**/*.js",
		"src/**/*.jsx"
	]
}