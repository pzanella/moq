{
	"name": "@moq/watch",
	"type": "module",
	"version": "0.2.0",
	"description": "Watch Media over QUIC broadcasts",
	"license": "(MIT OR Apache-2.0)",
	"repository": "github:moq-dev/moq",
	"exports": {
		".": "./src/index.ts",
		"./element": "./src/element.ts",
		"./ui": "./src/ui/index.tsx",
		"./support": "./src/support/index.ts",
		"./support/element": "./src/support/element.ts"
	},
	"sideEffects": [
		"./src/element.ts",
		"./src/ui/index.tsx",
		"./src/support/element.ts"
	],
	"scripts": {
		"build": "rimraf dist && vite build && tsc && bun ../common/package.ts",
		"check": "tsc --noEmit",
		"test": "bun test --only-failures",
		"release": "bun ../common/release.ts"
	},
	"dependencies": {
		"@moq/hang": "workspace:^",
		"@moq/lite": "workspace:^",
		"@moq/signals": "workspace:^",
		"@moq/ui-core": "workspace:^"
	},
	"devDependencies": {
		"@types/audioworklet": "^0.0.77",
		"@typescript/lib-dom": "npm:@types/web@^0.0.241",
		"esbuild": "^0.27.0",
		"rimraf": "^6.0.1",
		"solid-element": "^1.9.1",
		"solid-js": "^1.9.10",
		"typescript": "^5.9.2",
		"vite": "^7.3.1",
		"vite-plugin-solid": "^2.11.10"
	}
